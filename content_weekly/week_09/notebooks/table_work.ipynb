    "phrase = \"the_stealth_warrior\"\n",
    "\n",
    "def toCamelCase(text):\n",
    "    phrase = text.replace(\"_\",\" \")\n",
    "    phrase = phrase.split()\n",
    "    return phrase[0] + ''.join(word.capitalize() for word in phrase[1:])\n",
    "\n",
    "toCamelCase(phrase)"
